{
    "manifest_version": 2,
    "name": "Meet It",
    "version": "2.0.0",
    "description": "A simple, lightweight extension to take you back to your \"important\" Google Meet tab from your current \"more important\" tab.",
    "background": {
        "persistent": true,
        "scripts": ["src/background.js"]
    },
    "permissions": ["contextMenus", "activeTab", "tabs", "notifications"],
    "content_scripts": [
        {
          "matches": ["http://meet.google.com/*", "https://meet.google.com/*"],
          "js": ["src/content.js"],
          "all_frames": true,
          "run_at": "document_start"
        }
      ],
      "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'" 
}